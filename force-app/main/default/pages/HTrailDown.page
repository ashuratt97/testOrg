        <script type="text/javascript">
            j$ = jQuery.noConflict();
            j$(document).ready(function () {
                var label = j$("label[id$='afr_language']").text();

                j$("a[class*='ink']").each(function () {
                    var link_href = j$(this).attr('href');
                    var pattern = new RegExp(/[#]/);

                    if (pattern.test(link_href)) {
                        link_href = link_href.replace('#', '?lang=' + label + '#');
                    } else {
                        link_href = link_href + '?lang=' + label;
                    }

                    j$(this).attr('href', link_href);
                });
            });
        </script>
